{
  "name": "@packdir/language-en",
  "version": "0.1.4",
  "description": "English messages for react-admin, the frontend framework for building admin applications on top of REST/GraphQL services",
  "files": [
    "*.md",
    "dist",
    "src"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "build": "yarn run build-cjs && yarn run build-esm",
    "build-cjs": "rimraf ./dist/cjs && tsc --outDir dist/cjs",
    "build-esm": "rimraf ./dist/esm && tsc --outDir dist/esm --module es2015",
    "watch": "tsc --outDir dist/esm --module es2015 --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/packdir/language-en.git"
  },
  "keywords": [
    "English"
  ],
  "author": "Henry",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/packdir/language-en/issues"
  },
  "homepage": "https://github.com/packdir/language-en#readme",
  "dependencies": {
    "ra-core": "^4.13.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6"
  }
}
